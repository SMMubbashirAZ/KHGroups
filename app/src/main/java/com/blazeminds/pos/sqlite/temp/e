SELECT * FROM cust_pricing WHERE ((type_id = 10 OR type_id = 0)  AND (customer_id = 44898 OR customer_id = 0) ) AND (product_id = 689 OR product_id = 0) AND (brand_id = 1 OR brand_id = 0 )  AND (product_type_id = 35 OR product_type_id = 0)  AND ( (min_qty <= 1 OR (min_qty IS NULL) OR (min_qty = 'NULL') OR (min_qty = 0))  AND ( max_qty >= 1 OR max_qty IS NULL  OR (min_qty = 'NULL') OR (max_qty = 0)))   AND (multi = 1 OR multi = 0) ORDER BY type_id DESC, customer_id DESC, product_id DESC, brand_id DESC, product_type_id DESC;